{
    "manifest_version": 3,
    "name": "Chrome Terminal",
    "version": "1.0.0",
    "author": "jack200336@gmail.com",
    "chrome_url_overrides": {
        "newtab": "src/index.html"
    },
    "omnibox": {
        "keyword": "/"
    },
    "background": {
        "service_worker": "src/background.js"
    },
    "permissions": [
        "bookmarks",
        "activeTab",
        "scripting",
        "favicon"
    ],
    "action": {
        "default_popup": "src/popup.html"
    }
}